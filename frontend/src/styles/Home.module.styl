.jungleBackground
  background-image: url('/jungle-background.jpg')
  background-size: cover
  background-position: center
  position: relative

  &::before
    content: ''
    position: absolute
    top: 0
    right: 0
    bottom: 0
    left: 0
    background-color: rgba(0, 128, 0, 0.3)
    z-index: 1

  > *
    position: relative
    z-index: 2

.jungleText
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5)

